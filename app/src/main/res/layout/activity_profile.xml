<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FAFCFF"
    tools:context=".ui.home.HomeFragment">


    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="-70dp"
        android:src="@drawable/quadradinho2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/voltar"
        android:layout_width="37dp"
        android:layout_height="35dp"
        android:layout_marginStart="33dp"
        android:layout_marginTop="15dp"
        android:src="@drawable/seta"
        app:layout_constraintEnd_toStartOf="@+id/imageView15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView13" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="235dp"
        android:layout_height="254dp"
        android:layout_marginTop="5dp"
        android:src="@drawable/perfil_bolinha"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView13"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="42dp"
        android:layout_marginEnd="192dp"
        android:fontFamily="@font/raleway_light"
        android:text="Olá, Usuário"
        android:textColor="#4B6992"
        android:textSize="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView15" />

    <RelativeLayout
        android:id="@+id/spreadsheet"
        android:layout_width="334dp"
        android:layout_height="210dp"
        android:layout_marginTop="13dp"
        android:background="@drawable/card_profile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13"
        tools:ignore="MissingConstraints">


        <ImageView
            android:id="@+id/imageView16"
            android:layout_width="22dp"
            android:layout_height="28dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="30dp"
            android:layout_marginTop="22dp"
            app:srcCompat="@drawable/perfilvazio" />

        <ImageView
            android:id="@+id/imageView17"
            android:layout_width="26dp"
            android:layout_height="28dp"
            android:layout_below="@+id/imageView16"
            android:layout_alignParentStart="true"
            android:layout_marginStart="28dp"
            android:layout_marginTop="8dp"
            app:srcCompat="@drawable/cracha" />

        <ImageView
            android:id="@+id/imageView18"
            android:layout_width="24dp"
            android:layout_height="22dp"
            android:layout_below="@+id/imageView17"
            android:layout_alignParentStart="true"
            android:layout_marginStart="26dp"
            android:layout_marginTop="10dp"
            app:srcCompat="@drawable/email" />

        <ImageView
            android:id="@+id/imageView20"
            android:layout_width="29dp"
            android:layout_height="19dp"
            android:layout_below="@+id/imageView18"
            android:layout_alignParentStart="true"
            android:layout_marginStart="24dp"
            android:layout_marginTop="11dp"
            app:srcCompat="@drawable/calendario" />

        <ImageView
            android:id="@+id/imageView21"
            android:layout_width="22dp"
            android:layout_height="20dp"
            android:layout_below="@+id/imageView20"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="29dp"
            android:layout_marginTop="14dp"
            android:layout_marginBottom="26dp"
            app:srcCompat="@drawable/cracha2" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="155dp"
            android:layout_height="26dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="15dp"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="105dp"
            android:layout_toEndOf="@+id/imageView16"
            android:fontFamily="@font/monda"
            android:text="XXXX XXXXX XXX"
            android:textColor="#5F6570"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="138dp"
            android:layout_height="26dp"
            android:layout_below="@+id/textView14"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="13dp"
            android:layout_marginTop="11dp"
            android:layout_marginEnd="129dp"
            android:layout_toEndOf="@+id/imageView17"
            android:fontFamily="@font/monda"
            android:text="123456"
            android:textColor="#5F6570"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="180dp"
            android:layout_height="27dp"
            android:layout_below="@+id/textView15"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="14dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="87dp"
            android:layout_toEndOf="@+id/imageView18"
            android:fontFamily="@font/monda"
            android:text="xxxxxxxx@gmail.com"
            android:textColor="#5F6570"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="162dp"
            android:layout_height="26dp"
            android:layout_below="@+id/textView16"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="11dp"
            android:layout_marginTop="7dp"
            android:layout_marginEnd="98dp"
            android:layout_toEndOf="@+id/imageView20"
            android:fontFamily="@font/monda"
            android:text="12/12/2012"
            android:textColor="#5F6570"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/textView18"
            android:layout_width="176dp"
            android:layout_height="27dp"
            android:layout_below="@+id/textView17"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="13dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="94dp"
            android:layout_marginBottom="24dp"
            android:layout_toEndOf="@+id/imageView21"
            android:fontFamily="@font/monda"
            android:text="********789-10"
            android:textColor="#5F6570"
            android:textSize="20dp" />

    </RelativeLayout>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/sair"
        android:layout_width="320dp"
        android:layout_height="62dp"
        android:layout_marginTop="13dp"
        android:fontFamily="@font/monda"
        android:text="Sair"
        android:textSize="18dp"
        app:backgroundTint="@color/blue_scany"
        app:cornerRadius="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spreadsheet"
        app:layout_constraintVertical_bias="0.032" />


</androidx.constraintlayout.widget.ConstraintLayout>